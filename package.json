{
  "name": "swag-lab-ui-tests",
  "version": "1.0.0",
  "description": "Demo website automation using Cypress",
  "main": "index.js",
  "repository": "https://github.com/AutomationFrameworks/Cypress.git",
  "dependencies": {},
  "devDependencies": {
    "cypress": "^5.1.0",
    "@testing-library/cypress": "^7.0.5",
    "cypress-multi-reporters": "^1.5.0",
    "mocha": "^9.0.3",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "check": "prettier --check './**/*.{ts,js,json}'",
    "format": "prettier --write './**/*.{ts,js,json}'",
    "report:generate": "marge index.json --reportDir public --assetsDir public/assets --reportPageTitle index.html",
    "report:merge": "mochawesome-merge cypress/results/json/*.json > index.json",
    "test:chrome": "cypress run --browser chrome",
    "combine:report": "mochawesome-merge ./mochawesome-report/*.json -o index.json",
    "generate:report": "marge index.json --reportDir public --assetsDir public/assets --reportPageTitle index.html"
  },
  "author": "Ganesh Hubale",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AutomationFrameworks/Cypress/issues"
  },
  "homepage": "https://github.com/AutomationFrameworks/Cypress#readme"
}
